{
    "ver": "1.6.0",
    "authors": [
        "dylan"
    ],
    "contact": "http://kbase.us/contact-us/",
    "visible": true,
    "categories": ["inactive", "comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_genome_refs",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomes.Genome", "KBaseSearch.GenomeSet"]
            }
        },
        {
            "id": "desc",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text"
        }, 
        {
            "id": "output_tree_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.Tree" ],
		"is_output_name": true
            }
        },
        {
            "id": "genome_disp_name_config",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "obj_name" ],
            "field_type": "dropdown",
            "dropdown_options": {
	    	"options": [
				{
					"value": "obj_name",
					"display": "Object_Name",
					"id": "genome_disp_name_config-obj_name",
					"ui-name": "genome_disp_name_config-obj_name"
				},
				{
					"value": "obj_name_ver",
					"display": "Object_Name.version",
					"id": "genome_disp_name_config-obj_name_ver",
					"ui-name": "genome_disp_name_config-obj_name_ver"
				},
				{
					"value": "sci_name",
					"display": "Scientific Name",
					"id": "genome_disp_name_config-sci_name",
					"ui-name": "genome_disp_name_config-sci_name"
				},
				{
					"value": "obj_name_sci_name",
					"display": "Object_Name: Scientific Name",
					"id": "genome_disp_name_config-obj_name_sci_name",
					"ui-name": "genome_disp_name_config-obj_name_sci_name"
				},
				{
					"value": "obj_name_ver_sci_name",
					"display": "Object_Name.version: Scientific Name",
					"id": "genome_disp_name_config-obj_name_ver_sci_name",
					"ui-name": "genome_disp_name_config-obj_name_ver_sci_name"
				}
		]
            }
        },
        {
            "id": "skip_trimming",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
		    {
			"value": "0",
			"display": "DO trim MSA",
			"id": "skip_trimming-0",
			"ui-name": "skip_trimming-0"
		    },
		    {
			"value": "1",
			"display": "DON'T trim MSA",
			"id": "skip_trimming-1",
			"ui-name": "skip_trimming-1"
		    }
		]
	    }
        },
        {
            "id": "muscle_maxiters",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "16" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 1
            }
        },
        {
            "id": "muscle_maxhours",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 0.1
            }
        },
        {
            "id": "gblocks_trim_level",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0",
		"max_integer": "2"
            }
        },
        {
            "id": "gblocks_min_seqs_for_conserved",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "gblocks_min_seqs_for_flank",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "gblocks_max_pos_contig_nonconserved",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "8" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "gblocks_min_block_len",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "2"
            }
        },
        {
            "id": "gblocks_remove_mask_positions_flag",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_fastest",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_pseudo",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_gtr",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_wag",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_noml",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_nome",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "fasttree_cat",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "20" ],
